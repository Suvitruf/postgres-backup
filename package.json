{
  "name": "postgres-backup",
  "version": "0.0.1",
  "author": "Andrei Apanasik (Suvitruf)",
  "description": "A tool for backuping postgres databases",
  "keywords": [
    "postgres",
    "backup",
    "cli",
    "pg",
    "s3"
  ],
  "scripts": {
    "build": "tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json",
    "start": "ts-node lib/postgres-backup/cli.ts",
    "start:prod": "node dist/cjs/lib/postgres-backup/cli.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "typescript": "^4.5.2",
    "commander": "^8.3.0",
    "aws-sdk": "^2.1043.0",
    "date-fns": "^2.27.0",
    "node-cron": "^3.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "@types/node": "^14.18.0",
    "@types/node-cron": "^3.0.0"
  },
  "main": "./dist/cjs/lib/index.js",
  "module": "./dist/esm/lib/index.js",
  "bin": {
    "postgres_backup": "./dist/cjs/lib/postgres-backup/cli.js"
  },
  "license": "MIT"
}
